a="Prit"
print(a.upper())
print(a.lower())
print(a.replace("Prit","Pritam"))
print(a.split(" "))
print(a.capitalize())
print(a.center(50))
print(a.count("Prit"))
print(a.endswith("m"))
print(a.find("i"))
print(a.index("i"))
print(a.isalnum())
print(a.isalpha())
print(a.islower())
print(a.isprintable())
print(a.isspace())
print(a.istitle())
print(a.isupper())
print(a.startswith("P"))
print(a.swapcase())
print(a.title())
